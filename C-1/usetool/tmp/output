FUNCTION foo :
  PARAM a
  PARAM b

FUNCTION main :
  b := #0
  t1 := a + b
  t2 := t1 + #2
  t3 := t2 * #3
  t4 := #1 + t3
  a := t4
  ARG a
  t5 := #1 + #2
  ARG t5
  ARG b
  t6 := CALL foo
  ARG t6
  t7 := CALL foo
  IF FALSE a < b GOTO label1
  a := #0
  GOTO label2
LABEL label1 :
  b := #0
  IF FALSE a == b GOTO label3
  t8 := #2 * b
  a := t8
LABEL label3 :
LABEL label2 :
  a := #100
LABEL label4 :
  IF FALSE a > #0 GOTO label5
  b := a
  IF FALSE b != #0 GOTO label6
  t9 := a - #1
  a := t9
  GOTO label7
LABEL label6 :
  t10 := a \ #2
  a := t10
LABEL label7 :
  GOTO label4
LABEL label5 :
